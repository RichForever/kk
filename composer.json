{
  "name": "mm/starter",
  "type": "wordpress-theme",
  "description": "A modern Wordpress theme using Timber, ACF, Tailwind CSS and Alpine JS",
  "license": "MIT",
  "require": {
    "houke/acf-icon-picker": "dev-master",
    "timber/timber": "^2.0",
    "wpackagist-plugin/contact-form-7": "5.8.4",
    "wpackagist-plugin/duplicate-page": "4.5.3",
    "wpengine/advanced-custom-fields-pro": "^6.0"
  },
  "repositories": [
    {
      "type":"composer",
      "url":"https://connect.advancedcustomfields.com"
    },
    {
      "type":"composer",
      "url":"https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": ["type:wordpress-plugin"]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "hellonico/timber-dump-extension": "^1.0",
    "symfony/debug": "^4.4"
  }
}
